import acm.graphics.*;
import acm.program.*;
import java.awt.*;

public class Target extends GraphicsProgram {	
	public void run() {
	GOval oval = new GOval(100,50,72,72);
	oval.setFilled(true);
	oval.setColor(Color.RED);
	add(oval);
    GOval oval = new GOval(150,100,46,46);
    oval.setFilled(true);
    oval.setColor(Color.GREEN);
    add(oval);
		
		
	}
}
